# Local development environment for DJI Storefront (Next.js)

# Medusa backend (production API is fine for local frontend dev)
MEDUSA_BACKEND_URL=https://api.aidenlux.com

# Publishable key from Medusa Admin → Settings → Publishable API Keys
NEXT_PUBLIC_MEDUSA_PUBLISHABLE_KEY=pk_replace_me

# Default region for storefront routing
NEXT_PUBLIC_DEFAULT_REGION=us

# Google OAuth (popup)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=
NEXT_PUBLIC_ENABLE_GOOGLE_OAUTH_POPUP=true

# Strapi content API (optional for blog)
STRAPI_API_URL=https://content.aidenlux.com
STRAPI_API_TOKEN=

# ISR revalidate secret (optional for local)
REVALIDATE_SECRET=dev_only_secret

